<% include ./partials/header %>

<% blogs.forEach((blog) => { %>
    <div>
        <h2><%= blog.title %></h2>
        <img src="<%= blog.image %>" alt="" width=800px height=500px>
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
    </div>
<% }) %>

<% include ./partials/footer %>